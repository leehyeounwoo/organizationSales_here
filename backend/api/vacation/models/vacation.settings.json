{
  "kind": "collectionType",
  "collectionName": "vacations",
  "info": {
    "name": "vacation",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "vacationDate": {
      "type": "date"
    },
    "vacationReason": {
      "type": "richtext"
    },
    "adminId": {
      "type": "string"
    },
    "comment": {
      "type": "string"
    },
    "viewStatus": {
      "type": "boolean"
    },
    "gotowork": {
      "model": "gotoworks",
      "via": "vacation"
    },
    "user": {
      "via": "vacations",
      "plugin": "users-permissions",
      "model": "user"
    },
    "vacationType": {
      "type": "string"
    },
    "vacationStatus": {
      "type": "enumeration",
      "enum": [
        "agree",
        "disagree",
        "waiting"
      ]
    }
  }
}
